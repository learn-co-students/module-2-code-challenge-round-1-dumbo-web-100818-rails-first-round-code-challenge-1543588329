<h2>All Heroines</h2>

<%= form_tag("/heroines", method: "get") do %>
  <%= label_tag("Search by superpower:") %>
  <%= text_field_tag(:power_id) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
<%= link_to "New Heroine", new_heroine_path %>
