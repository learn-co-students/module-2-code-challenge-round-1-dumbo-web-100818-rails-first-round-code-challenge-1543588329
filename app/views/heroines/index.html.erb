<h2>All Heroines</h2>


<%= form_for heroines_path, method: "get" do |f|%>
  <%= f.label :q, "Search by Superpower" %>
  <%= f.text_field :q %>
  <%= f.submit "Search" %>
<% end  %>



<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
