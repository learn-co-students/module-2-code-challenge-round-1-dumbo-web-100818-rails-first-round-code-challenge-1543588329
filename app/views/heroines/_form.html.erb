
<%= render 'layouts/errors', :object=>@heroine %>


<%=form_for @heroine do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>

  <%= f.label :super_name %><br>
  <%= f.text_field :super_name %><br><br>

  <%= f.label :power_id %><br>
  <%= f.collection_select(:power_id, @powers, :id, :name) %><br><br>

  <%= f.submit %>

<% end %>
